<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/assisted_cooking_microwave_oven_graph"
    app:startDestination="@id/microwaveAssistedModesRecycleView">


    <fragment
        android:id="@+id/microwaveAssistedModesRecycleView"
        android:name="com.whirlpool.cooking.MWO.MicrowaveAssistedModesRecycleView"
        android:label="MicrowaveAssistedModesRecycleView" >
        <action
            android:id="@+id/action_microwaveAssistedModesRecycleView_to_microwaveOvenAssistedModesOptionSelection"
            app:destination="@id/microwaveOvenAssistedModesOptionSelection" />
        <action
            android:id="@+id/action_microwaveAssistedModesRecycleView_to_MWOCookingStatus"
            app:destination="@id/MWOCookingStatus" />
        <action
            android:id="@+id/action_microwaveAssistedModesRecycleView_to_microwaveCookTimeNumpad"
            app:destination="@id/microwaveCookTimeNumpad" />
    </fragment>

    <fragment
        android:id="@+id/microwaveOvenAssistedModesOptionSelection"
        android:name="com.whirlpool.cooking.MWO.MicrowaveOvenAssistedModesOptionSelection"
        android:label="MicrowaveOvenAssistedModesOptionSelection" >
        <action
            android:id="@+id/action_microwaveOvenAssistedModesOptionSelection_to_MWOCookingStatus"
            app:destination="@id/MWOCookingStatus" />
    </fragment>
    <fragment
        android:id="@+id/MWOCookingStatus"
        android:name="com.whirlpool.cooking.MWO.MWOCookingStatus"
        android:label="MWOCookingStatus" >
        <action
            android:id="@+id/action_MWOCookingStatus_to_microwaveCookTimeNumpad"
            app:destination="@id/microwaveCookTimeNumpad" />
    </fragment>
    <fragment
        android:id="@+id/microwaveCookTimeNumpad"
        android:name="com.whirlpool.cooking.MWO.MicrowaveCookTimeNumpad"
        android:label="MicrowaveCookTimeNumpad" >
        <action
            android:id="@+id/action_microwaveCookTimeNumpad_to_MWOCookingStatus"
            app:destination="@id/MWOCookingStatus" />
    </fragment>
</navigation>