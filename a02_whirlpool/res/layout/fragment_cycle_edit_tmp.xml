<?xml version="1.0"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="480dp"
        android:layout_height="302dp"
        android:background="@drawable/background_fragment_bottomsheet_tile">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/frame_1197"
            android:layout_width="442dp"
            android:layout_height="299dp"
            android:layout_marginStart="19dp"
            android:layout_marginTop="16dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/frame_1093"
                android:layout_width="442dp"
                android:layout_height="40dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <ImageView
                    android:id="@+id/cycle_edit_back_arrow"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:background="@drawable/icon_40px_backarrow"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageView
                    android:id="@+id/cycle_edit_right_icon"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginStart="402dp"
                    android:background="@drawable/icon_40px_info"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <com.whirlpool.hmi.uicomponents.widgets.grid.GridView
                android:id="@+id/cycleeditRecycler"
                android:layout_width="440dp"
                android:layout_height="302dp"
                android:layout_marginStart="1dp"
                android:layout_marginTop="52dp"
                android:clipChildren="false"
                app:item_spacing="16"
                app:layout_constraintTop_toTopOf="parent"
                app:number_of_columns="2"
                android:visibility="gone"
                tools:ignore="MissingConstraints" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/frame_311"
                android:layout_width="440dp"
                android:layout_height="135dp"
                android:layout_marginStart="1dp"
                android:layout_marginTop="55dp"
                android:visibility="visible"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/frame_309"
                    android:layout_width="440dp"
                    android:layout_height="60dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/tile_scrollpopup_icon"
                        android:layout_width="212dp"
                        android:layout_height="60dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                        <View
                            android:id="@+id/rectangle_1020"
                            android:layout_width="212dp"
                            android:layout_height="60dp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <TextView
                            android:id="@+id/cook_time"
                            style="@style/W35TileRegular20"
                            android:layout_width="137dp"
                            android:layout_height="22dp"
                            android:layout_marginStart="53dp"
                            android:layout_marginTop="19dp"
                            android:fontFamily="@font/roboto_regular"
                            android:fontWeight="400"
                            android:gravity="left|top"
                            android:includeFontPadding="false"
                            android:lineHeight="22.0dp"
                            android:textColor="@color/color_whp_text_gray"
                            android:textSize="20sp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <ImageView
                            android:id="@+id/icon_30px_cooktimer_yellow"
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            android:layout_marginStart="15dp"
                            android:layout_marginTop="18dp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <ImageView
                        android:id="@+id/tile_scrollpopup_icon_1"
                        android:layout_width="212dp"
                        android:layout_height="60dp"
                        android:layout_marginStart="228dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />
                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/frame_310"
                    android:layout_width="440dp"
                    android:layout_height="60dp"
                    android:layout_marginTop="75dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/tile_scrollpopup_icon_2"
                        android:layout_width="212dp"
                        android:layout_height="60dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                        <View
                            android:id="@+id/rectangle_1020_1"
                            android:layout_width="212dp"
                            android:layout_height="60dp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <TextView
                            android:id="@+id/cook_time_1"
                            style="@style/W35TileRegular20"
                            android:layout_width="137dp"
                            android:layout_height="22dp"
                            android:layout_marginStart="53dp"
                            android:layout_marginTop="19dp"
                            android:fontFamily="@font/roboto_regular"
                            android:fontWeight="400"
                            android:gravity="left|top"
                            android:includeFontPadding="false"
                            android:lineHeight="22.0dp"
                            android:textColor="@color/color_whp_text_gray"
                            android:textSize="20sp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <ImageView
                            android:id="@+id/icon_30px_cooktimer_yellow_1"
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            android:layout_marginStart="15dp"
                            android:layout_marginTop="18dp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <ImageView
                        android:id="@+id/tile_scrollpopup_icon_1_1"
                        android:layout_width="212dp"
                        android:layout_height="60dp"
                        android:layout_marginStart="228dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>

            <Button
                android:id="@+id/button_textlong"
                style="@style/W35ButtonBold20"
                android:layout_width="440dp"
                android:layout_height="44dp"
                android:layout_marginStart="1dp"
                android:layout_marginTop="205dp"
                android:background="@drawable/background_frame_1197_button_textlong"
                android:textAllCaps="false"
                app:backgroundTint="@null"
                android:letterSpacing="0"
                android:text="@string/text_stop_cooking"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>