<?xml version="1.0"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="36dp"
        android:layout_marginRight="36dp">

        <View
            android:id="@+id/vector_83"
            android:layout_width="399dp"
            android:layout_height="1dp"
            android:layout_marginTop="68dp"
            android:background="#c4c4c4"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/group_1182"
            android:layout_width="407dp"
            android:layout_height="31dp"
            android:layout_marginTop="20dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/text"
                style="@style/Tile35Regular20"
                android:layout_width="248dp"
                android:layout_height="30dp"
                android:layout_marginTop="1dp"
                android:fontFamily="@font/roboto_regular"
                android:fontWeight="400"
                android:gravity="left|center_vertical"
                android:includeFontPadding="false"
                android:lineHeight="22.0dp"
                android:text="Standard brightness"
                android:textColor="#363636"
                android:textSize="20sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/text_1"
                android:layout_width="181dp"
                android:layout_height="22dp"
                android:layout_marginStart="192dp"
                android:layout_marginTop="5dp"
                android:fontFamily="@font/roboto_regular"
                android:fontWeight="400"
                android:gravity="right|center_vertical"
                android:includeFontPadding="false"
                android:lineHeight="22.0dp"
                android:text="Eco"
                android:textColor="#6b6b6b"
                android:textSize="20sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/icon_30px_forwardarrow"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginStart="377dp"
                android:src="@drawable/icon_30px_forward_arrow"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
