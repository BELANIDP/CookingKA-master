<?xml version="1.0"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/background_fragment_bottomsheet_tile">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/frame_312"
            android:layout_width="480dp"
            android:layout_height="236dp"
            android:layout_marginStart="00dp"
            android:layout_marginTop="19dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:id="@+id/image_neutralbg"
                android:layout_width="480dp"
                android:layout_height="236dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <com.whirlpool.hmi.uicomponents.widgets.buttons.NavigationButton
                android:id="@+id/active_button_right"
                style="@style/ButtonOtherBold15"
                android:layout_width="110dp"
                android:layout_height="40dp"
                android:layout_marginStart="40dp"
                android:layout_marginTop="162dp"
                android:background="@drawable/background_buttons_button_cta_ghost_light_text"
                android:fontWeight="700"
                android:gravity="center_horizontal|center_vertical"
                android:lineHeight="16.0dp"
                android:text="Cancel"
                android:textColor="#363636"
                android:textSize="16sp"
                app:backgroundTint="@null"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <com.whirlpool.hmi.uicomponents.widgets.buttons.NavigationButton
                android:id="@+id/button_cta_primary_active"
                style="@style/ButtonOtherBold15"
                android:layout_width="105dp"
                android:layout_height="40dp"
                android:layout_marginStart="335dp"
                android:layout_marginTop="162dp"
                android:background="@drawable/background_buttons_button_cta_primary_active"
                android:fontWeight="700"
                android:gravity="center_horizontal|center_vertical"
                android:lineHeight="16.0dp"
                android:text="Pause"
                android:textColor="#363636"
                android:textSize="16sp"
                app:backgroundTint="@null"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/text_cooktime"
                style="@style/RobotoReg42"
                android:layout_width="156dp"
                android:layout_height="34dp"
                android:layout_marginStart="151dp"
                android:layout_marginTop="104dp"
                android:fontFamily="@font/roboto_regular"
                android:fontWeight="400"
                android:gravity="center_horizontal|center_vertical"
                android:letterSpacing="-0.01"
                android:lineHeight="34.0dp"
                android:textColor="#363636"
                android:textSize="37sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/icon_30px_endofcooking"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginStart="23dp"
                android:layout_marginTop="39dp"
                android:background="@drawable/icon_30px_cyclecomplete"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/kitchen_timer_complete"
                android:includeFontPadding="false"
                android:fontFamily="@font/roboto_bold"
                android:layout_width="362dp"
                android:layout_height="33dp"
                android:layout_marginStart="69dp"
                android:layout_marginTop="38dp"
                android:fontWeight="400"
                android:gravity="center_horizontal|center_vertical"
                android:lineHeight="32.0dp"
                android:text="Kitchen Timer Complete"
                android:textColor="#363636"
                android:textSize="30sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/group_1049"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginStart="424dp"
                android:layout_marginTop="13dp"
                android:visibility="gone"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <View
                    android:id="@+id/rectangle_927"
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:background="#363636"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <View
                    android:id="@+id/rectangle_928"
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:background="#363636"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <ImageView
                android:id="@+id/icon_50px_backarrow"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:background="@drawable/icon_50px_backarrow"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <com.whirlpool.hmi.uicomponents.widgets.grid.GridView
                android:id="@+id/bottomsheetRecycler"
                android:layout_width="400dp"
                android:layout_height="55dp"
                app:number_of_columns="2"
                app:item_spacing="11"
                android:layout_marginStart="19dp"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="200dp" />

        </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout></layout>