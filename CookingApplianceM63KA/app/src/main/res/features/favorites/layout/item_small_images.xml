<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@color/common_solid_black">

    <ImageView
        android:layout_width="@dimen/fav_image_height_width"
        android:layout_height="@dimen/fav_image_height_width"
        android:id="@+id/imageView"
        android:layout_marginEnd="@dimen/fav_image_padding"
        android:layout_marginStart="@dimen/fav_image_padding"
        tools:background="@drawable/background"
        android:importantForAccessibility="no" />

</FrameLayout>
