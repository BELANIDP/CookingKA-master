<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/outer_ring">
        <shape
            android:shape="ring"
            android:thickness="@dimen/thickness_radio_button_outer_ring"
            android:innerRadius="@dimen/inner_radius_radio_button_outer_ring"
